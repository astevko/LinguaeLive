<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat' ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator" ui:generateLocales="default"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
  <ui:style src="../style.css" />

	<g:FocusPanel ui:field="focusPanel">
		<g:DeckPanel width="100%" height="100%" ui:field="deckPanel">
			<g:NumberLabel styleName="{style.formPromptData}"  ui:field="editLabel"/>
			<g:TextBox  styleName="{style.formPromptEdit}"   ui:field="editBox"/>
		</g:DeckPanel>
	</g:FocusPanel>
</ui:UiBinder> 