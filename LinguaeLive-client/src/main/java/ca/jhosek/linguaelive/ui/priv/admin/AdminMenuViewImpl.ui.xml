<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat' ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator" ui:generateLocales="default"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:style src="../../../style.css" />

	<g:HTMLPanel>
		<g:MenuBar addStyleNames="{style.menuBar}" >
			<g:MenuItem styleName="{style.menuItem}"  text="Admin Home" ui:field="adminHome">
				<ui:attribute name='text' />
				  <ui:msg description='Admin Home'>Admin Home</ui:msg>
  			</g:MenuItem>
			<g:MenuItem styleName="{style.menuItem}"  text="View Users" ui:field="queryUsers">
				<ui:attribute name='text' />
				  <ui:msg description='View Users'>View Users</ui:msg>
  			</g:MenuItem>
			<g:MenuItem styleName="{style.menuItem}"  text="View Courses" ui:field="courses">
				<ui:attribute name='text' />
				  <ui:msg description='View Courses'>View Courses</ui:msg>
  			</g:MenuItem>
		</g:MenuBar>
	</g:HTMLPanel>
</ui:UiBinder> 