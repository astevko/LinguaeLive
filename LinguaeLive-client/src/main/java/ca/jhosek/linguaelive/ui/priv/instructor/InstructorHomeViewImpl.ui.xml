<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat' ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator" ui:generateLocales="default"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

  <ui:style src="../../../style.css" />

	<g:HTMLPanel styleName="{style.panel}"  height="20em" >		
		<g:HTMLPanel styleName="{style.leftAlign}"  >
			<div class="{style.sectionHeader}" ><ui:msg description="Your Courses">Your Courses</ui:msg></div>
			<g:HorizontalPanel ui:field="coursesPanel" >
				<g:Label styleName="{style.sectionItalic}"><ui:msg description="Loading...">Loading...</ui:msg></g:Label>
			</g:HorizontalPanel>			
			<div class="{style.sectionHeader}" ><ui:msg description="Course Link Invites">Course Link Invites</ui:msg></div>
			<div class="{style.sectionHeader2}" ><ui:msg description="Sent">Sent</ui:msg></div>
			<g:FlowPanel ui:field="pendingLinkedCoursesPanel" >
				<g:Label styleName="{style.sectionItalic}"><ui:msg description="Loading...">Loading...</ui:msg></g:Label>
			</g:FlowPanel>
			<div class="{style.sectionHeader2}" ><ui:msg description="Received">Received</ui:msg></div>
			<g:FlowPanel ui:field="openLinkedCoursesPanel" >
				<g:Label styleName="{style.sectionItalic}"><ui:msg description="Loading...">Loading...</ui:msg></g:Label>
			</g:FlowPanel>
		</g:HTMLPanel>				
	</g:HTMLPanel>
</ui:UiBinder> 