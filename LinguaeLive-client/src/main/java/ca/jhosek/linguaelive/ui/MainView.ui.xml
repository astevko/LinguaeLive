<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat' ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator" ui:generateLocales="default"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:style src="../style.css" />
		
	<g:HTMLPanel styleName="{style.verytop}" >
		<!-- logo panel region -->		
		<!-- <g:SimplePanel  styleName="{style.logoPanel}"  text="Home" ui:field="logoPanel"/>	Added by Rumman -->
		
		<div class="{style.logoPanel}" >
			<a href="/#index:" ><img src="/images/header.jpg" alt="LinguaeLive.ca" /></a>
		</div> 
		
		<!-- user panel region -->
		<g:SimplePanel styleName="{style.userPanel}" ui:field="userPanel" />
		<!-- menu panel region -->
		<g:SimplePanel styleName="{style.menuPanel}" ui:field="menuPanel" />

		<!-- center panel region -->
		<g:SimplePanel styleName="{style.centerPanel}" ui:field="centerPanel" />

		<!-- footer panel region -->
		<g:HTMLPanel styleName="{style.footerPanel}" ui:field="footerPanel" >
			<a href="/index.html?#privacy:"><ui:msg description="privacy statement">privacy statement</ui:msg></a>
			<font color="#d35d46">&nbsp;|&nbsp;</font>
			<a href="/index.html?#terms:"><ui:msg description="terms of service">terms of service</ui:msg></a>
			<font color="#d35d46">&nbsp;|&nbsp;</font>
			<a href="/index.html?#contactus:"><ui:msg description="contact us">contact us</ui:msg></a>
			<div class="copyright">Copyright &copy; 2011 - 2014 LinguaeLive, LLC </div>
			<form action="https://www.paypal.com/cgi-bin/webscr" method="post">
				<input type="hidden" name="cmd" value="_donations"/>
				<input type="hidden" name="business" value="jhosek@queensu.ca"/>
				<input type="hidden" name="lc" value="US"/>
				<input type="hidden" name="item_name" value="Linguaelive"/>
				<input type="hidden" name="no_note" value="0"/>
				<input type="hidden" name="currency_code" value="CAD"/>
				<input type="hidden" name="bn" value="PP-DonationsBF:btn_donateCC_LG.gif:NonHostedGuest"/>
				<input type="image" src="https://www.paypalobjects.com/en_US/i/btn/btn_donateCC_LG.gif" border="0" name="submit" alt="PayPal - The safer, easier way to pay online!"/>
				<img alt="" border="0" src="https://www.paypalobjects.com/en_US/i/scr/pixel.gif" width="1" height="1"/>
			</form>
			
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder>

 